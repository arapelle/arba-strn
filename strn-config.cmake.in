# strn-config.cmake - package configuration file

@PACKAGE_INIT@

include(${CMAKE_CURRENT_LIST_DIR}/strn.cmake)

get_target_property(strn-CONFS strn IMPORTED_CONFIGURATIONS)
if(strn-CONFS)
    foreach(strn-CONF ${strn-CONFS})
        # Get shared
        get_target_property(shared-path strn IMPORTED_LOCATION_${strn-CONF})
        get_filename_component(shared-name ${shared-path} NAME)
        # Get static
        get_target_property(static-path strn-static IMPORTED_LOCATION_${strn-CONF})
        get_filename_component(static-name ${static-path} NAME)
        message(STATUS "Found strn ${strn-CONF}: (found version \"${strn_VERSION}\"): ${shared-name} ${static-name}")
    endforeach()
endif()
