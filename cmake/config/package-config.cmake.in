@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(arba-cppx 0.1.0 CONFIG)

include(${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@-targets.cmake)
check_required_components(@PROJECT_NAME@-targets)

if(NOT TARGET "@PROJECT_NAMESPACE@::@PROJECT_BASE_NAME@@LIBRARY_TYPE_POSTFIX@")
    add_library("@PROJECT_NAMESPACE@::@PROJECT_BASE_NAME@@LIBRARY_TYPE_POSTFIX@" ALIAS @PROJECT_TARGET_NAME@)
endif()

message(STATUS "Found package @PROJECT_NAME@ @PROJECT_VERSION@")
